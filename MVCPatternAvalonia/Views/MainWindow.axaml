<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:MVCPatternAvalonia.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:MVCPatternAvalonia.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MVCPatternAvalonia.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="MVCPatternAvalonia">
	 <StackPanel>
        <TextBox x:Name="NameTextBox" Watermark="Name" Margin="10"/>
        <TextBox x:Name="PhoneNumberTextBox" Watermark="Phone" Margin="10"/>
        <Button Content="Add Contact" x:Name="AddContactCommand" Margin="10" Click="AddContact_Click"/>
        <ListBox x:Name="ContactListBox" Margin="10"/>
    </StackPanel>
</Window>
